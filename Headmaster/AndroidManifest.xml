	<!-- 允许应用读写系统设置项。 -->	
	<uses-permission android:name="VIBRATE"/>
	<!-- 允许应用震动。 -->	
	<uses-permission android:name="MOUNT_UNMOUNT_FILESYSTEMS"/>	
	<!-- 允许应用挂载/卸载 外部文件系统。 -->
	<uses-permission android:name="ACCESS_NETWORK_STATE"/>	
	<!-- 允许应用获取网络信息状态，如当前的网络连接是否有效。 -->
